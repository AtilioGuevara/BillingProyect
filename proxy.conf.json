{
  "/api/auth/**": {
    "target": "https://accounts.beckysflorist.site",
    "secure": true,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Host": "accounts.beckysflorist.site"
    }
  },
  "/api/bill/**": {
    "target": "https://bill.beckysflorist.site",
    "secure": true,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/bill": "/bill/api"
    },
    "headers": {
      "Host": "bill.beckysflorist.site"
    }
  }
}